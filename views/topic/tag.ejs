

<%- include ../share/header %>
<div class="row">
    <div class="col-md-2 text-center">
	</div>


  <div class="col-md-7">
	<% posts.forEach(function (post, index) { %>
	<div class="topic-cell"> 
	  <p><h3><a href="/topic/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h3></p>
	 
	  <p class="info" style="color:#969696">
	    作者：<a href="/user/<%= post.name %>"><%= post.name %></a>
	  </p>
	  <p class="demop"><%- post.post %></p>
	   <p class="text-right" style="color:#969696">
	    <%= post.time.minute %>
	  </p>
	  </div>
	<% }) %>
	</div>

	<div class="col-md-3 text-center">


	</div>
</div>


<%- include ../share/footer %>